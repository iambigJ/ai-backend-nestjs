{
    "name": "ai-backend-template",
    "version": "1.0.0",
    "description": "is just sample of nestjs backend to help community",
    "author": "ALI.Javadi",
    "private": true,
    "license": "MIT",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "npx typeorm migration:run -d dist/typeorm.migration-config.js && node dist/main",
        "typeorm": "npx typeorm-ts-node-commonjs",
        "migration:generate": "npm run typeorm migration:generate -- -d src/typeorm.migration-config.ts",
        "migration:create": "npm run typeorm migration:create",
        "migration:run": "npm run typeorm migration:run -- -d src/typeorm.migration-config.ts",
        "migration:revert": "npm run typeorm migration:revert -- -d src/typeorm.migration-config.ts",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@golevelup/nestjs-rabbitmq": "^4.0.0",
        "@nestjs/cache-manager": "^2.1.0",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.1.1",
        "@nestjs/core": "^10.0.0",
        "@nestjs/event-emitter": "^2.0.2",
        "@nestjs/jwt": "^10.1.1",
        "@nestjs/mapped-types": "*",
        "@nestjs/microservices": "^10.2.7",
        "@nestjs/passport": "^10.0.2",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/platform-socket.io": "^10.2.8",
        "@nestjs/swagger": "^7.1.14",
        "@nestjs/typeorm": "^10.0.0",
        "@nestjs/websockets": "^10.2.8",
        "@types/luxon": "^3.3.4",
        "amqp-connection-manager": "^4.1.14",
        "amqplib": "^0.10.3",
        "archiver": "^6.0.1",
        "bcryptjs": "^2.4.3",
        "cache-manager": "^5.1.4",
        "cache-manager-redis-store": "^2.0.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "kavenegar": "^1.1.4",
        "luxon": "^3.4.3",
        "nestjs-soap": "^3.0.2",
        "otp-generator": "^4.0.1",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "pg": "^8.11.3",
        "pg-hstore": "^2.3.4",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1",
        "sharp": "^0.32.6",
        "socket.io": "^4.7.2",
        "typeorm": "^0.3.17",
        "typeorm-transactional": "^0.5.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@types/amqplib": "^0.10.3",
        "@types/archiver": "^6.0.1",
        "@types/bcryptjs": "^2.4.5",
        "@types/cache-manager-redis-store": "^2.0.1",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.2",
        "@types/kavenegar": "^1.1.2",
        "@types/multer": "^1.4.9",
        "@types/node": "^20.3.1",
        "@types/otp-generator": "^4.0.1",
        "@types/passport-local": "^1.0.37",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.5.0",
        "prettier": "^3.0.0",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": ".",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "moduleNameMapper": {
            "src/(.*)": "<rootDir>/src/$1"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
